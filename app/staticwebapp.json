{
    "routes": [
        {
            "route": "/*",
            "rewrite": "/index.html"
        }
    ],
    "mimeTypes": {
        ".js": "text/javascript",
        ".css": "text/css"
    },
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    }
}