{
    "routes": [
        {
            "route": "/*",
            "rewrite": "/index.html",
            "headers": {
                "Cache-Control": "no-cache",
                "Content-Type": "text/javascript; charset=utf-8"
            }
        }
    ],
    "mimeTypes": {
        ".js": "text/javascript",
        ".css": "text/css"
    },
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    }
}